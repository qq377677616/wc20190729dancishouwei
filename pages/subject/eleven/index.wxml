                    <view class="icon-one {{datas[0].pic.isY==1?'yes':datas[0].pic.isY==2?'no':''}}" data-index="0" data-name="pic" data-type="{{datas[0].pic.type}}" bindtap="select"><image src="{{datas[0].pic.answer}}"></image></view>
