                    <view class="icon-two {{datas[1].pic.isY==1?'yes':datas[1].pic.isY==2?'no':''}}" data-index="1" data-name="pic" data-type="{{datas[1].pic.type}}" bindtap="select"><image src="{{datas[1].pic.answer}}"></image></view>
                </view>
                <view>
