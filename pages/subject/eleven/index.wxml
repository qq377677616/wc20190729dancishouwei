                <view>
                    <view class="icons icon-one {{datas[0].isY==1?'yes':datas[0].isY==2?'no':''}} {{datas[0].ischeck?'bor':''}}" data-index="0" data-name="pic" data-type="{{datas[0].type}}" bindtap="select"><image class="{{datas[0].isTip?'tran':''}}" src="{{datas[0].pic}}"></image></view>
                    <view class="flex">
                      <view class="contents content-one {{datas[1].isY==1?'yes':datas[1].isY==2?'no':''}} {{datas[1].isTip?'font':''}} {{datas[1].ischeck?'bor':''}}" data-index="1" data-name="txt" data-type="{{datas[1].type}}" bindtap="select">{{datas[1].txt}}</view>
                    </view>
                    
                </view>
                <view class="center">
                  <view class="flex">
                    <view class="contents content-two {{datas[3].isY==1?'yes':datas[3].isY==2?'no':''}} {{datas[3].isTip?'font':''}} {{datas[3].ischeck?'bor':''}}" data-index="3" data-name="txt" data-type="{{datas[3].type}}" bindtap="select">{{datas[3].txt}}</view>
                  </view>
                    <view class="icons icon-two {{datas[2].isY==1?'yes':datas[2].isY==2?'no':''}} {{datas[2].ischeck?'bor':''}}" data-index="2" data-name="pic" data-type="{{datas[2].type}}" bindtap="select"><image class="{{datas[2].isTip?'tran':''}}" src="{{datas[2].pic}}"></image></view>
                </view>
                <view>
                    <view class="icons icon-three {{datas[4].isY==1?'yes':datas[4].isY==2?'no':''}} {{datas[4].ischeck?'bor':''}}" data-index="4" data-name="pic" data-type="{{datas[4].type}}" bindtap="select"><image class="{{datas[4].isTip?'tran':''}}" src="{{datas[4].pic}}"></image></view>
                    <view class="flex">
                      <view class="contents content-three {{datas[5].isY==1?'yes':datas[5].isY==2?'no':''}} {{datas[5].isTip?'font':''}} {{datas[5].ischeck?'bor':''}}" data-index="5" data-name="txt" data-type="{{datas[5].type}}" bindtap="select">{{datas[5].txt}}</view>
                    </view>
                </view>
            </view>
        </view>
    </view>
    <view class="tips">
      <image mode="widthFix" src="{{resourcesUrl}}tips.png" bindtap="tips"></image>
    </view>
    <tips bind:click="isTips" bind:clicks="cane" isShow="{{isShow}}" star="1"></tips>
</view>



