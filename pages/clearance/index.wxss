/* pages/clearance/index.wxss */
.clearance-title{
  position: relative;
  z-index: 10;
}
.x1{
  position: absolute;
  /* top: 40rpx;
  left: 140rpx; */
  z-index: 5;
  width: 60rpx;
  animation: myfirst1 2.5s;
  animation-fill-mode: forwards;
}
.x2{
  position: absolute;
  /* top: -30rpx;
  left: 300rpx; */
  animation: myfirst2 2.5s;
  animation-fill-mode: forwards;
  z-index: 5;
  width: 40rpx;
}
.x3{
  position: absolute;
  /* top: 45rpx;
  right: 150rpx; */
  animation: myfirst3 2.5s;
  animation-fill-mode: forwards;
  z-index: 5;
  width: 60rpx;
  transform: rotate(30deg);
}
@keyframes myfirst1
{
0% {top: 40rpx;left:160rpx;opacity: 0;}
50% {top: -60rpx;left: 140rpx;opacity: 1;}
75% {top: -60rpx;left: 140rpx;opacity: 0}
100% {top: -60rpx;left: 140rpx;opacity: 1}
}
@keyframes myfirst2
{
0% {top: 55rpx;left:300rpx;opacity: 0;}
50% {top: -150rpx;left: 300rpx;opacity: 1;}
75% {top: -150rpx;left: 300rpx;opacity: 0;}
100% {top: -150rpx;left: 300rpx;opacity: 1;}
}
@keyframes myfirst3
{
0% {top: 120rpx;right:170rpx;opacity: 0;}
50% {top: -65rpx;right: 150rpx;opacity: 1;}
75% {top: -65rpx;right: 150rpx;opacity: 0;}
100% {top: -65rpx;right: 150rpx;opacity: 1;}
}
.airc{
  width: 240rpx;
  position: absolute;
  top: -100rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 999;
  animation: floats 1.3s ease-in-out 2.7s infinite alternate;
}
@keyframes floats{
  0%   {transform:translate(-50%,0px)}
  100% {transform:translate(-50%,10rpx)}
}
.clearance-title >view{
    background-image: url("https://flynew.oss-cn-hangzhou.aliyuncs.com/game/wechat/szq/danci/images/Mycity/page19_2.png");
    background-size: 100%;
    background-repeat: no-repeat;
    box-sizing: border-box;
    width: 557rpx;
    height: 310rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    padding-top: 160rpx;
    margin: 220rpx auto 0 auto;
    position: relative;
    z-index: 10;
}
.clearance-title >view image{
    width: 66rpx;
    height: 66rpx;
}
.clearance-title >view text{
    font-size: 32rpx;
    color: #ffd149;
}
.clearance-select{
    display: flex;
    width: 55%;
    align-items: center;
    justify-content: space-between;
    margin: 50rpx auto 0 auto;
}
.clearance-select button::after{
  border: none;
}
.clearance-select button{
  background: none;
  border: none;
}
.xing{
  width: 50rpx;
  position: absolute;
  right: 145rpx;
  top: 550rpx;
  z-index: 5;
}
.clearance-select image{
    width: 108rpx;
    height: 108rpx;
}
.clearance-btn{
    padding-top: 20rpx;
}
.clearance-btn image{
    margin: 30rpx auto;
    display: block;
    width: 363rpx;
    height: 104rpx;
}

.dig-member{
    position: fixed;
    top: 0;
    bottom: 0;
    width: 100vw;
    background-color: rgba(0,0,0,.8);
}
.dig-member >view{
    background-image: url("https://flynew.oss-cn-hangzhou.aliyuncs.com/game/wechat/szq/danci/images/clearance/page19_14.png");
    background-repeat: no-repeat;
    background-size: 100%;
    width: 641rpx;
    height: 435rpx;
    margin: 341rpx auto 0 auto;
}
.dig-member >image{
    display: block;
    width: 121rpx;
    height: 122rpx;
    margin: 41rpx auto 0 auto;
}
.dig-member >view >image{
    display: block;
    width: 441rpx;
    height: 52rpx;
    margin: 0 auto;
    padding-top: 70rpx;
    padding-bottom: 20rpx;
}
.description{
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding:0 100rpx 15rpx 106rpx;
    box-sizing: border-box;
}
.description >image{
    width: 155rpx;
    height: 67rpx;
}
.description text{
  padding-left: 20rpx;
  color: #fff;
}
.month{
  width: 139rpx;
  height: 24rpx;
}
.btn{
  width: 154rpx;
  height: 66rpx;
  padding-left: 70rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  color: #fff;
}
.description view{
    display: flex;
    align-items: center;
}
.description view >image:nth-child(1){
    width: 56rpx;
    height: 45rpx;
}
.dig-job{
    background-color: rgba(0,0,0,.8);
    position: fixed;
    top: 0;
    bottom: 0;
    width: 100vw;
}
.dig-job >view{
    background-image: url("https://flynew.oss-cn-hangzhou.aliyuncs.com/game/wechat/szq/danci/images/clearance/page19_12.png");
    background-repeat: no-repeat;
    background-size: 100%;
    width: 616rpx;
    height: 334rpx;
    position: relative;
    margin: 300rpx auto 0 auto;
}
.dig-job >view image{
    width: 267rpx;
    height: 83rpx;
    position: absolute;
    bottom: 20rpx;
    left: calc(50% - 133.5rpx);
}

.zhe{
  width: 100%;
  height: 100%;
  background: black;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 5;
}
.monster{
  width: 200rpx;
  position: fixed;
  top: 45%;
  left: 50%;
  transform: translate(-50%,-48%);
  z-index: 10;
  animation: float1 .8s ease-in-out 1.7s infinite alternate;
}
@keyframes float1{
  0%   {transform:translate(-50%,-48%)}
  100% {transform:translate(-50%,-52%)}
}