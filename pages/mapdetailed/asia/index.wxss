scroll-view{
    width:100%;
    position: absolute;
    left:0;
    top:0;
    font-size: 0;
    height: 100vh;
    background-image: url("https://flynew.oss-cn-hangzhou.aliyuncs.com/game/wechat/szq/danci/images/mapdetailed/asia/asia-hai.png");
    background-size: cover;
}
image{
  width: 100%;
  height: 100%;
}
.line{
  width: 1770rpx;
  position: relative;
  top: 260rpx;
  left: 30rpx;
}
.line .limg{
  width: 100%;
}
.map-bg{
    width:1977rpx;
    height: 100% !important;
    position: absolute;
    left:0;
    top:0;
}
.back{width:323rpx;
    position: fixed;
    left:50%;
    transform: translateX(-50%);
    bottom:50rpx;
    z-index: 9;
}
.aircraft{
    position: absolute;
    width:111rpx;
    height:77rpx;
    z-index: 200000;
}
