.ul .li{
  position: absolute;
  /* width:300rpx; */
    height: 40rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between
}
.ul .li text{
  display: inline-block;
  width: 200rpx;
  font-size: 26rpx;
  color: #fff;
  text-shadow: #002bb5 2rpx 4rpx 5rpx;
  /* -webkit-text-stroke: 1px #9B88E2; */
}

.ul .li .icon{
    width: 40rpx;
    /* height: 40rpx; */
    margin-right: 10rpx;
    animation: floats 1.6s ease-in-out 2.9s infinite alternate;
}
@keyframes floats{
  0%   {transform:translate(0,-2px)}
  100% {transform:translate(0,6rpx)}
}
