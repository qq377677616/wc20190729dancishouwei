/**index.wxss**/
page{
  width: 100%;
  height: 100%;
  overflow: hidden;
  font-family: "times";
}
.index{
  width: 100%;
    height: 100%;
    background: url("https://flynew.oss-cn-hangzhou.aliyuncs.com/game/wechat/szq/danci/images/index/page1_jpg.jpg") no-repeat;
    background-size: cover;
}
/* 图片变灰 */
.img{
  filter: grayscale(100%);
}
.title {
    padding:75rpx 30rpx 0 30rpx;
}
.title image:nth-child(1){
    width: 141rpx;
    height: 77rpx;
    padding-left: 20rpx;
    padding-bottom: 30rpx;
}
.title image:nth-child(2){
    width: 699rpx;
    height: 157rpx;
    padding-bottom: 60rpx;
}
.title image:nth-child(3){
    display: block;
    width: 501rpx;
    height: 348rpx;
    margin: 0 auto;
    animation: floats 1.3s ease-in-out 2.7s infinite alternate;
}
@keyframes floats{
  0%   {transform:translate(0,0px)}
  100% {transform:translate(0,10rpx)}
}
.index .btn{
    margin: 0 auto;
    display: block;
    width: 323rpx;
    height:115rpx;
    padding-top: 30rpx;
}

.fake-authorization{
    position: fixed;
    top: 0;
    bottom: 0;
    width: 100vw;
    background: rgba(0,0,0,.8);

}
.fake-authorization .content{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    height: 400rpx;
    width: 600rpx;
    background-color: white;
    border-radius: 5px;
}
.fake-authorization .content .header{
    font-size: 32rpx;
    line-height: 100rpx;
    text-align: center;
    border-bottom:1px #f4f4f4 solid;
}
.fake-authorization .content .toast{
    font-size: 28rpx;
    color: #96999e;
    line-height: 200rpx;
    text-align: center;
    border-bottom:1px #f4f4f4 solid;
}
.fake-authorization .content .btn{
    display: flex;
    align-items: center;
}
.fake-authorization .content .btn text, .fake-authorization .content .btn button{
    width: 50%;
    line-height: calc(100rpx - 2px);
    text-align: center;
    border-right: 1px #f4f4f4 solid;
    font-size: 30rpx;
}
.fake-authorization .content .btn button{
    color: #36b05a;
}
.fake-authorization .content .btn button:after{
    border: none !important;
}
.zhe{
  width: 100%;
  height: 100%;
  background: black;
  opacity: 0.8;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 20;
}
.popup{
  width: 658rpx;
  height: 502rpx;
  background-image: url(https://game.flyh5.cn/resources/game/wechat/szq/danci/images/index/first.png);
  background-size: cover;
  position: fixed;
  top: 45%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 25;
}
.btns{
  width: 270rpx;
  height: 85rpx;
  /* line-height: 75rpx; */
  background: none;
  background-image: url(https://game.flyh5.cn/resources/game/wechat/szq/danci/images/index/rece.png);
  background-size: cover;
  /* color: #fff;
  font-size: 36rpx;
  font-weight: 600; */
  position: absolute;
  left: 50%;
  bottom: 20rpx;
  transform: translateX(-50%);
}
.btns::after{
  border: none;
}
.zhes{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 50;
}
.skip{
  font-size: 32rpx;
  color: #fff;
  position: fixed;
  top: 30rpx;
  right: 40rpx;
  z-index: 55;
}
.cont{
  width: 100%;
  height: 80%;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 51;
}
.cont image{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.cont view{
  width: 100%;
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  font-size: 60rpx;
  color: #fff;
  font-weight: bold;
}
