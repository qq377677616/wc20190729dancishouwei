/* components/topicHerd/index.wxss */

.img{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  animation: float5 1.8s;
}
@keyframes float5{
  0%   {left: 0;}
  100% {left: -100%;}
}
.img image{
  max-width: 100%;
}